<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%=title%></title>
    <link rel="stylesheet" href="style1.css">
</head>

<body>
    <div class="menu">
        <div class="menu-item ib" id="file">File</div>
        <div class="menu-item ib" id="home">Home</div>

    </div>
    <div class="menu-options">
        <div class="menu-options-item" id="file-options">
            <div class="new ib">New</div>
            <div class="open ib">Open</div>
            <div class="save ib">Save</div>
        </div>
        <div class="menu-options-item" id="home-options"></div>
    </div>

    <div class="formula-bar" > Formula Bar
        <input type="text" id="address-container">
        <input type="text">
    </div>
    <div class="cell-container">

        <div class="top-left-cell">
        </div>
        <div class="top-row">
            <%for(let i=0;i<cols;i++){%>
            <div class="cell">
                <%=String.fromCharCode(i+65)%>
            </div>
            <%}%>
</div>
<div class="left-col">
    <%for(let i=1;i<=rows;i++){%>
            <div class="cell">
                <%=i%>
            </div>
            <%}%>
</div>
<div class="grid">
        <%for(let i = 1;i <= rows;i++){%>
            <div class="row">
                <%for(let j = 0; j<cols ;j++){%>
                <div class="cell" contenteditable="true" rid=<%=i%> cid=<%=j%>>
                </div>
                <%}%>
    </div>
    <%}%>
            </div>
        </div>
    </div>
    <div class="sheets">Sheets
    </div>
<script src="index.js"></script>
</body>

</html>